{
"update_url": "https://clients2.google.com/service/update2/crx",

  "manifest_version": 3,
  "name": "Exam Calculator - IITM Assignments",
  "version": "1.1",
  "description": "Use the calculator provided in exams during IITM assignments instantly",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "48": "icons/icon48.png",
    "96": "icons/icon96.png"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "popup.html",
        "css/calclayout.css",
        "js/jquery-1.8.0.min.js",
        "js/jquery-ui.min.js",
        "js/oscZenoedited.js"
      ],
      "matches": ["<all_urls>"]
    }
  ]
}
